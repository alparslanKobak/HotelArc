@model AppUser
@{
    ViewData["Title"] = "Delete";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<h1>Delete @Model.UserName</h1>

<h3>Are you sure you want to delete this?</h3>


<form method="post" asp-route-id="@Model.AppUserId" asp-controller="AppUsers" asp-action="Delete">
<input type="hidden" asp-for="AppUserId" />

    <button type="submit" class="btn btn-danger">Delete</button> |
<a asp-action="Index" asp-controller="AppUsers">Back to List</a>
</form>


